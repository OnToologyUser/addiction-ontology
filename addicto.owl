        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> ex-smoker</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> ex-smoker vaper</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> former smoker</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> vaper</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Current e-cigarette user</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The time period and frequency of past tobacco smoking use should also be defined,</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ex-smoking e-cigarette user</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000775 -->


    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000787 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000787">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000777"/>

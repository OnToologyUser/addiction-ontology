        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PWUD</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">person with opioid use disorder</rdfs:label>
    </owl:Class>
    



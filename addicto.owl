        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Non-user</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The time scale of non use must be defined, e.g., lifetime non-user or not a current user of psychoactive substances.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">non-psychoactive substance user</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000782 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000782">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000772"/>

        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> dependent drinker</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> harmful drinker</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> person with alcohol dependence</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> risky drinker</obo:IAO_0000118>

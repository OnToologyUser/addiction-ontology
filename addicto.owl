        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Injecting drug user</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IDU</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">person who injects drugs</rdfs:label>

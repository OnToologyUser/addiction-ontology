                <owl:someValuesFrom rdf:resource="http://addictovocab.org/ADDICTO_0000303"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A product derived from tobacco plants for ingestion of constituents of tobacco, not including products that have a medicinal licence.</obo:IAO_0000115>

        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Heavy smoker</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">chain smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000771 -->


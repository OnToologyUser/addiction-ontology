        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000194"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A brand of e-cigarette products that are manufactured by a particular tobacco company under a particular name.</obo:IAO_0000115>

        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Never smoker is often defined as a person who has smoked fewer than 20 cigarettes  in their lifetime.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">never smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000780 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000780">
        <rdfs:subClassOf rdf:resource="http://semanticscience.org/resource/SIO_000498"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person who does not currently report smoking tobacco.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> non-smoker</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ex-smoker</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term nono-smoker is included as a synonym but is discouraged from use. Not currently smoking also needs to be further defined by providign details of time since last smoking and frequency of smoking.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">not a current smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000781 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000781">
        <rdfs:subClassOf rdf:resource="http://semanticscience.org/resource/SIO_000498"/>

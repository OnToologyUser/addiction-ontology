        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A smokeless tobacco-containing product that is designed for oral use, in which air- or fire-cured burley tobacco leaf is treated with a tar-like tobacco leaf extract and sometimes sweeteners and other flavourings, then twisted into rope-like strands that are dried and braided.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> chaw</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> chew</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> roll</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">twist chewing tobacco</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Twist is handmade by commercial manufacturers, and users typically cut off a piece, place it in the mouth, and chew.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">twist</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0001015 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0001015">

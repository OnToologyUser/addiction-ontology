        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ever-tobacco smoker who has participated in tobacco smoking within the past year.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">past-year smoker</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This definition is commonly used in outputs from the Smoking Toolkit Study. It refers to those who affirm smoking any tobacco products within the past 12 months. For more info see: www.smokinginengland.info</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">past-year tobacco smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000783 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000783">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000513"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A psychoactive substance user who reports using substances.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> drug user</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> substance user</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PWUD</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use should be further defined into time and frequency.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">person who uses drugs</rdfs:label>
    </owl:Class>
    



        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Poverty</obo:IAO_0000119>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Definition changed to one that is ontologically OK.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Poverty</rdfs:label>
    </owl:Class>
    



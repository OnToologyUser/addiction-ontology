        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TSNA</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tobacco-specific nitrosamine</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000314 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000314">

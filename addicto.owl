        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> alcohol consumer</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Drinker</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">alcohol user</rdfs:label>
    </owl:Class>
    



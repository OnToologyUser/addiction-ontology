        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> current smoker</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> ex e-cigarette user</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> ex-vaper</obo:IAO_0000118>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Smoker</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The time period and frequency of past e-cigarette use should also be defined,</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ex e-cigarette user current smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000774 -->


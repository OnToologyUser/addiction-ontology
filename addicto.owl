    <!-- http://addictovocab.org/ADDICTO_0000257 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000257">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000279"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/CHEBI_18723"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A product that contains nicotine for ingestion by the user.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">nicotine product</obo:IAO_0000118>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation with &apos;nicotine&apos; is &apos;has part&apos; and not &apos;contains&apos; because contains is reserved for spatially enclosing a separate entity.</obo:IAO_0000232>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class overlaps with tobacco-containing product but is distinct from it as tobacco products can can have the nicotine removed and nicotine-containing products may not have any other tobacco constituents in them .</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">nicotine-containing product</rdfs:label>
    </owl:Class>
    



        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Current substance user</obo:IAO_0000118>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The term current must be defined further and frequency of use should also be recorded for greater clarification.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">current psychoactive substance user</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000772 -->


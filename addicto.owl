        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ex-smoker</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">former regular smoker</rdfs:label>
    </owl:Class>
    



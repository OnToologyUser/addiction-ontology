        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Substance use disorder</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000402 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000402">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An emotion directly attributing an affective response to an object, event or experience. It includes liking and disliking.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Targeted emotion</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000403 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000403">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DOID_4"/>

    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000137 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000137">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>

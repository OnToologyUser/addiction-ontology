        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tobacco brand</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cigarette brand</rdfs:label>
    </owl:Class>
    



        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tobacco smoke</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000315 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000315">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000212"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An e-cigarette that is pen shaped.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pen-like e-cigarette</obo:IAO_0000118>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vape pen</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000316 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000316">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000279"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
                <owl:someValuesFrom rdf:resource="http://addictovocab.org/ADDICTO_0000317"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A product that produces for inhalation by a person a vapour or aerosol by heating a substance that may be either liquid or solid.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vaping device</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000317 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000317">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000279"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A part of a vaping device.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vaping device part</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000319 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000319">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000127"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A consumption behaviour in which the substance ingested includes a psychoactive drug.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Drug use behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000320 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000320">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_037000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An intervention that realises a government policy.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Public policy intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000321 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000321">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000011"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A planned process in which a service provider performs a task  for a service consumer.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Service</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000322 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000322">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000324"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mortality in a population in which the deaths in question are at least partly caused by the consumption of alcohol by people in that population.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Expressed in terms of number or proportion of deaths or population attributable fraction.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alcohol-related mortality</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000324 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000324">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000525"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A population attribute that reflects the number of deaths within a population within a time frame.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mortality</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000325 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000325">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000525"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The health of a population</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Population health</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000326 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000326">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000027"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A data item that is the estimated number of years that a person can expect to live, adjusted downwards according to how far the assessed quality of life of that person is less than what is judged to be optimal.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">QALY</obo:IAO_0000118>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">No appropriate ontological definition found. Definition based on https://en.wikipedia.org/wiki/Quality_of_life</obo:IAO_0000232>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The issue of who does the judging would need to be specified and the basis for the judgement.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Quality-adjusted life year</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000327 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000327">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000328"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Treatment demand that is influenced by the availability of the treatment to to population.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supply-induced treatment demand</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000328 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000328">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000525"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A quality of a population that involves the number of people in the population who seek a treatment.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Treatment demand</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000341 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000341">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000027"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A data item that is about a person and signifies that the person has been diagnosed with alcohol use disorder within the past 12 months.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">12-month diagnosis of alcohol use disorder</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000342 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000342">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000016"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person who is exhibiting the attribute of abstinence from engagement in a behaviour.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Abstainer</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000343 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000343">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000515"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A personal role that is the extent to which a person can receive a service.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Access to a service</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000344 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000344">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000444"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An environmental disposition that is is the extent to which an environment enables people in that environment to receive addiction treatment</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Accessibility of addiction treatment</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000345 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000345">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000443"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An environmental disposition that is the extent to which an environment enables people in that environment to acquire naloxone.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Accessibility of naloxone</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000346 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000346">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000348"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acute effect of using a product on a person in which use of the product involves ingestion of a psychoactive substance.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acute effect of ingesting a substance on a person</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000347 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000347">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000346"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acute effect of ingesting a substance on a person in which the substance is alcohol.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acute effect of ingesting alcohol on a person</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000348 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000348">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process involving a person that results directly from,and within a short time period of, use by the person of a product.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acute effect of using a product on a person</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000349 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000349">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000033"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disposition towards repeated episodes of abnormally high levels of motivation to engage in a behaviour, acquired as a result of engaging in the behaviour, where the behaviour results in risk or occurrence of serious net harm.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dependence</obo:IAO_0000118>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The aim is to include dispositions about which there is a consensus that they constitute addiction while avoiding making a disputed statement about whether addiction necessarily involves disordered brain mechanisms .</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Addiction</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000350 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000350">
        <rdfs:subClassOf rdf:resource="http://humanbehaviourchange.org/ontology/BCIO_042000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A behaviour that is addictive in a significant proportion of of people who engage in it.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There is not a consensus on what what counts as a significant proportion but in practice in common usage it can be as low as 10%.</obo:IAO_0000232>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Currently accepted as addictive behaviours are: alcohol consumption, use of nicotine or tobacco-containing products, opioid use, stimulant use, cannabis use, sedative use, steroid use,and  gambling. Some experts believe that online gaming should be added.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Addictive behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000351 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000351">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000016"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A mental disorder that results in repeated episodes of abnormally high levels of motivation to engage in a behaviour, acquired as a result of engaging in the behaviour, where the behaviour results in risk or occurrence of serious net harm.</obo:IAO_0000115>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dependence disorder</obo:IAO_0000118>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is separated fropm &apos;addiction&apos; to provide a parent term for the range of &apos;disorders&apos; relating to substance use that are defined as such and about which, therefore, there cannot be legitimate dispute.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Addictive disorder</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000352 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000352">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MF_0000016"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person who has reached maturity.</obo:IAO_0000115>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">For use in research contexts the minimum age needs to be specified.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Adult</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000353 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000353">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATO_0001018"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A time quality inhering in a bearer by virtue of how much time elapsed between birth and a certain point.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000011</obo:IAO_0000119>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A slight wording change from &apos;A time quality inhering in a bearer by virtue of how long the bearer has existed.&apos; This captures more precisely that the amount is &apos;time&apos; rather than using the word &apos;long&apos; and that it relates to give time point and that existence is defined as time of birth.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Age</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000354 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000354">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A material entity  that is either a human being, an aggregate of humans or an organisation and that has the capability of intentionality.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The definition has been altered from &apos;A material entity  which is active in a process or has the relevant capabilities of intentionality.&apos; to allow for the fact that organisations are not aggregates of people in this ontology and that agents need not actually be active in a process.</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Agent</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000356 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000356">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000383"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Injury that is caused at least in part by the consumption of alcohol by a person.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alcohol-related injury</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000357 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000357">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process that is causally influenced by consumption of alcohol by a person.</obo:IAO_0000115>
        <obo:IAO_0000232 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Logically defined class: its members will be inferred by the OWL reasoner - it does not have to be a parent of another class</obo:IAO_0000232>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alcohol-related outcome</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000358 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000358">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000520"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A traffic crash that is at least partially caused by intoxication resulting from consumption of alcohol.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alcohol-related traffic crash</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000359 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000359">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000003"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A fiat part of a complex phenomenon that has salience in a particular context.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aspect</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000360 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000360">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A process that has as output a material entity that is the material basis for an abnormal homeostasis dispostion.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bodily harm</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000361 -->


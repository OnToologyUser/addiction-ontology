        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Predominant smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000395 -->

    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000395">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000375"/>

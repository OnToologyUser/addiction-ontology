        <rdfs:label>liquid commodity</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000538 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000538">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000827"/>
        <obo:IAO_0000115>A product part that is a component of a tobacco-containing product.</obo:IAO_0000115>
        <rdfs:label>tobacco-containing product part</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000624 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000624">

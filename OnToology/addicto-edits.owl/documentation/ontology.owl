        <rdfs:label>self-report questionnaire</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000156 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000156">

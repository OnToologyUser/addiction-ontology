        <rdfs:label>mint flavoured e-liquid</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000625 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000625">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000239"/>

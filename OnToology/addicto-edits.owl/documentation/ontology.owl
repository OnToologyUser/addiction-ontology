        <rdfs:label>menthol flavoured e-liquid</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000626 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000626">

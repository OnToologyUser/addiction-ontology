        <rdfs:label>drink driving legislation</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000478 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000478">
        <rdfs:subClassOf rdf:resource="http://addictovocab.org/ADDICTO_0000490"/>

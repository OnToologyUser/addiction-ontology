            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
                <owl:someValuesFrom rdf:resource="http://addictovocab.org/ADDICTO_0000303"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>A tobacco-containing product that is a part of a heated tobacco-containing product.</obo:IAO_0000115>
        <obo:IAO_0000232>The formal definition looks clunky but is formally correct.</obo:IAO_0000232>
        <rdfs:label>heated tobacco-containing product part</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000536 -->



        <rdfs:label>adverse event profile of a product</rdfs:label>
    </owl:Class>
    


    <!-- http://addictovocab.org/ADDICTO_0000192 -->


    <owl:Class rdf:about="http://addictovocab.org/ADDICTO_0000192">
